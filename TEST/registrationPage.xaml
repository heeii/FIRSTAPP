<?xml version="1.0" encoding="utf-8" ?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TEST.registrationPage"
             Title="registrationPage">
    <VerticalStackLayout Spacing="10" Margin="5">
            <Editor x:Name="Login_Place"
            Text=""
            Placeholder="Login"
            HeightRequest="100" />
            <Editor x:Name="Password_Place"
            Text=""
            Placeholder="Password"
            HeightRequest="100" />
        <Editor x:Name="Password_Check_Place"
            Text=""
            Placeholder="Check Password"
            HeightRequest="100" />
        <Editor x:Name="email_Place"
            Text=""
            Placeholder="e-mail"
            HeightRequest="100" />
        <Button x:Name="registration_btn"
                Text="Registration"
                Clicked="registration_btn_Clicked"/>
    </VerticalStackLayout>
</ContentPage>